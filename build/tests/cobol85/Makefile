duration.log:
	@echo "Collecting durations for each module..."
	@echo "Duration for modules: $(MODULES)" > $@
	@echo "" >> $@
	@for m in $(MODULES); do \
		cat "$$m/duration.txt" >> $@; \
	done
	@echo Done

