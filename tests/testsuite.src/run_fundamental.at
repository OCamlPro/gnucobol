           IF SPACE      >= FLDX500    DISPLAY '  SPACE >= "X5"!'.

           MOVE SPACES TO FLDXX
           IF FLDXX      =  "    "     CONTINUE
           ELSE                        DISPLAY '! "XX" <> "     "'.
           MOVE ALL "A" TO FLDXX
           IF FLDXX      =  "AA  "     CONTINUE
