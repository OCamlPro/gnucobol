           ELSE                        DISPLAY '! "X" > " "'.
           IF FLDX       <  " "        DISPLAY '  "X" < " "!'.
           IF FLDX       >= " "        CONTINUE
           ELSE                        DISPLAY '! "X" >= " "'.
           IF FLDX       <= " "        DISPLAY '  "X" <= " "!'.

           IF " "        <  FLDX       CONTINUE
           ELSE                        DISPLAY '! " " < "X"'.
           IF " "        >  FLDX       DISPLAY '  " " > "X"!'.
           IF " "        <= FLDX       CONTINUE
           ELSE                        DISPLAY '! " " <= "X"'.
           IF " "        >= FLDX       DISPLAY '  " " >= "X"!'.


