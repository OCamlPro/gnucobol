           IF "0"        <= FLD1X      CONTINUE
           ELSE                        DISPLAY '! "0" <= "1 "'.
           IF "0"        >= FLD1X      DISPLAY '  "0" >= "1 "!'.

      *> special check for SPACE-optimization and extra spaces
           IF FLDX500    >  SPACE      CONTINUE
           ELSE                        DISPLAY '! "X5" > SPACE'.
           IF FLDX500    <  SPACE      DISPLAY '  "X5" < SPACE!'.
           IF FLDX500    >= SPACE      CONTINUE
