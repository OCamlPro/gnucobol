           ELSE                        DISPLAY '! "X" < x"FF"'.
           IF FLDX       >  x"FF"      DISPLAY '  "X" > x"FF"!'.
           IF FLDX       <= x"FF"      CONTINUE
           ELSE                        DISPLAY '! "X" <= x"FF"'.
           IF FLDX       >= x"FF"      DISPLAY '  "X" >= x"FF"!'.

           IF x"FF"      >  FLDX       CONTINUE
           ELSE                        DISPLAY '! x"FF" > "X"'.
