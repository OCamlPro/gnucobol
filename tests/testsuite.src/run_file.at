       PROCEDURE DIVISION.

       MAINFILE.
           ACCEPT START-TIME FROM TIME.
           ACCEPT CMD-LINE FROM COMMAND-LINE.
           IF CMD (1:2) = "NO"
              GO TO MAINTEST.
           DISPLAY "IX_OPTIONS"   UPON ENVIRONMENT-NAME
           IF CMD = 'OCI'
           OR CMD = 'OLK'
             DISPLAY "format=oci,dups_ahead=always"
                         UPON ENVIRONMENT-VALUE
             DISPLAY "Using OCI"
             DISPLAY "RL_OPTIONS"  UPON ENVIRONMENT-NAME
             DISPLAY "format=oci" UPON ENVIRONMENT-VALUE
           ELSE IF CMD = "DI"
             DISPLAY "format=disam,file_log=yes"
                         UPON ENVIRONMENT-VALUE
             DISPLAY "Using DISAM"
           ELSE IF CMD = "VB"
             DISPLAY "format=vbisam,file_log=yes"
                         UPON ENVIRONMENT-VALUE
             DISPLAY "Using VBISAM"
           ELSE IF CMD = "VC"
             DISPLAY "format=vbcisam,file_log=yes"
                         UPON ENVIRONMENT-VALUE
             DISPLAY "Using VBCISAM"
           ELSE IF CMD = "BDB"
             DISPLAY "Using BDB"
             DISPLAY "format=bdb,dups_ahead=always"
                     UPON ENVIRONMENT-VALUE
           ELSE IF CMD = "ODB"
             DISPLAY "Using ODBC"
             DISPLAY "format=odbc,dups_ahead=always"
                     UPON ENVIRONMENT-VALUE
           ELSE
             DISPLAY "Using Built-in INDEXED handler"
           END-IF.

       MAINTEST.
           PERFORM LOADFILE.
           PERFORM REWRFILE.
           PERFORM READFILE.
           STOP RUN.

       LOADFILE.
           DISPLAY "Loading Indexed data file."
                            UPON CONSOLE.

           OPEN OUTPUT TSPFILE
           IF  CUST-STAT NOT = "00"
           AND CUST-STAT NOT = "05"
               DIVIDE STAT-1 BY 256 GIVING BYTE-1 REMAINDER BYTE-2
               DISPLAY "Error " CUST-STAT " " BYTE-1 " " BYTE-2
                       " opening 'testsql' file" UPON CONSOLE
               STOP RUN
           END-IF.

           PERFORM 1000-LOAD-RECORD
                        VARYING SUB FROM 1 BY 1
                          UNTIL SUB > MAX-SUB.

           DISPLAY "Sample data file load complete."
                            UPON CONSOLE.
           CLOSE TSPFILE.

       REWRFILE.
           DELETE FILE TSPFILE.
           DISPLAY "Deleted file: " CUST-STAT.
           IF  CUST-STAT NOT = "00"
               COMMIT
               DELETE FILE TSPFILE
               DISPLAY "2nd delete file: " CUST-STAT
           END-IF.
           COMMIT.

           OPEN OUTPUT TSPFILE
           DISPLAY "Now Rewrite file: " CUST-STAT.
           IF  CUST-STAT NOT = "00"
           AND CUST-STAT NOT = "05"
               DISPLAY "Error " CUST-STAT " :" STAT-X2
                       ": opening 'testsql' file" UPON CONSOLE
               STOP RUN
           END-IF.

           MOVE 'N' TO DO-REWRITE.
           PERFORM 1000-LOAD-RECORD
                        VARYING SUB FROM 1 BY 1
                          UNTIL SUB > MAX-SUB.
           CLOSE TSPFILE.
           DISPLAY "Closed indexed file: " CUST-STAT.
           OPEN I-O TSPFILE
           DISPLAY "Reopen I-O indexed file: " CUST-STAT.
           MOVE 'Y' TO DO-REWRITE.
           PERFORM 1000-LOAD-RECORD
                        VARYING SUB FROM 1 BY 2
                          UNTIL SUB > MAX-SUB.
           MOVE 'N' TO DO-REWRITE.

           DISPLAY "Sample data file rewrite complete."
                            UPON CONSOLE.
           CLOSE TSPFILE.
           DISPLAY "rewrite Commit.".
           COMMIT.
           DISPLAY "done Commit.".

      *---------------------------------------------------------------*
      *         LOAD A RECORD FROM DATA TABLES                        *
      *---------------------------------------------------------------*

       1000-LOAD-RECORD.

           MOVE SPACES                       TO TSPFL-RECORD.
           MOVE DATA-CUST-NUM      (SUB)     TO CM-CUST-NUM.
           MOVE CM-CUST-NUM                  TO TSPFL-KEY.
           MOVE DATA-COMPANY       (SUB)     TO CM-COMPANY.
           MOVE DATA-ADDRESS-1     (SUB)     TO CM-ADDRESS-1.
           MOVE DATA-ADDRESS-2     (SUB)     TO CM-ADDRESS-2.
           MOVE DATA-ADDRESS-3     (SUB)     TO CM-ADDRESS-3.
           MOVE DATA-TELEPHONE     (SUB)     TO CM-TELEPHONE.
           MOVE DATA-DP-MGR        (SUB)     TO CM-DP-MGR.
           MOVE DATA-MACHINE       (SUB)     TO CM-MACHINE.
           MOVE DATA-NO-TERMINALS  (SUB)     TO CM-NO-TERMINALS.

           IF  ODD-RECORD
               MOVE "8417"                   TO CM-DISK
               MOVE "1600 BPI"               TO CM-TAPE
               MOVE "01MB"                   TO CM-MEMORY
           ELSE
               MOVE "8470"                   TO CM-DISK
               MOVE "6250 BPI"               TO CM-TAPE
               MOVE "03GB"                   TO CM-MEMORY.
           IF  SUB = 1 OR 6
               MOVE "2417"                   TO CM-DISK
               MOVE "549  mmm"               TO CM-TAPE.

           IF DO-REWRITE = 'Y'
               IF  SUB NOT = 1 AND SUB NOT = 6
                 MOVE "REWRITE"            TO CM-DISK
               END-IF
               REWRITE TSPFL-RECORD
               IF  CUST-STAT NOT = "00"
               AND CUST-STAT NOT = "02"
                   DISPLAY "REWRITE: " SUB ", " TSPFL-KEY
                           ", Disk=" CM-DISK
                           ", Phone=" DATA-TELEPHONE (SUB)
                           ", Machine=" DATA-MACHINE (SUB)
                           ", Status: " CUST-STAT UPON CONSOLE
               END-IF
           ELSE
               WRITE TSPFL-RECORD
               IF  CUST-STAT NOT = "00"
               AND CUST-STAT NOT = "02"
                   DISPLAY "WRITE: " TSPFL-KEY ", Status: "
                           CUST-STAT UPON CONSOLE
               END-IF
           END-IF.

       READFILE.
           MOVE ZERO TO REC-NUM
           DISPLAY "Another Commit."
           COMMIT.
           DISPLAY "READ indexed file" UPON CONSOLE.
           OPEN I-O TSPFILE.
           IF  CUST-STAT = "41"
               DISPLAY "Note got " CUST-STAT " open I-O "
               CLOSE TSPFILE
               OPEN I-O TSPFILE
           END-IF.
           IF  CUST-STAT NOT = "00"
               DISPLAY "ERROR " CUST-STAT " Opening indexed file"
                                             UPON CONSOLE
               STOP RUN
           END-IF.
           MOVE SPACES                       TO TSPFL-RECORD.
           MOVE DATA-CUST-NUM (4)            TO CM-CUST-NUM.
           MOVE DATA-COMPANY  (4)            TO CM-COMPANY.
           READ  TSPFILE WITH NO LOCK
           IF CUST-STAT NOT = "00"
               DISPLAY "Error " CUST-STAT " on direct read of "
                                CM-CUST-NUM UPON CONSOLE
               CLOSE TSPFILE
               STOP RUN
           END-IF.
           DISPLAY "Key:   " CM-CUST-NUM " is " CM-COMPANY '.'
           READ  TSPFILE NEXT RECORD WITH NO LOCK
           IF CUST-STAT NOT = "00"
               DISPLAY "Error " CUST-STAT " on read next of file"
                                UPON CONSOLE
               CLOSE TSPFILE
               STOP RUN
           END-IF.
           DISPLAY "Key:N  " CM-CUST-NUM " is " CM-COMPANY '.'.
           READ  TSPFILE PREVIOUS RECORD WITH NO LOCK
           IF CUST-STAT NOT = "00"
               DISPLAY "Error " CUST-STAT " on read previous of file"
                                UPON CONSOLE
               CLOSE TSPFILE
               STOP RUN
           END-IF.
           DISPLAY "Key:P  " CM-CUST-NUM " is " CM-COMPANY '.'.
           MOVE DATA-CUST-NUM (8)            TO CM-CUST-NUM.
           MOVE DATA-COMPANY  (8)            TO CM-COMPANY.
           READ  TSPFILE WITH LOCK
           DISPLAY "Key: L " CM-CUST-NUM " is " CM-COMPANY '.'.
           MOVE "Boo" TO CM-COMPANY
           REWRITE TSPFL-RECORD
           DISPLAY "Rewrote " CM-CUST-NUM
                    " as " CM-COMPANY " Sts:" CUST-STAT
           READ  TSPFILE NEXT RECORD WITH LOCK
           IF CUST-STAT NOT = "00"
               DISPLAY "Error " CUST-STAT " on read next lock of file"
                                UPON CONSOLE
               CLOSE TSPFILE
               STOP RUN
           END-IF.
           DISPLAY "Key:NL " CM-CUST-NUM " is " CM-COMPANY '.'.
           MOVE CM-CUST-NUM TO SAV-KEY.
           DELETE  TSPFILE
           IF CUST-STAT NOT = "00"
               DISPLAY "Error " CUST-STAT " on DELETE " CM-CUST-NUM
                                UPON CONSOLE
               CLOSE TSPFILE
               STOP RUN
           END-IF.
           DISPLAY "Key:   " CM-CUST-NUM " was deleted.".
           DELETE  TSPFILE
           DISPLAY "Error " CUST-STAT " on DUP DELETE."
                                UPON CONSOLE.
           READ  TSPFILE
           IF CUST-STAT NOT = "00"
               DISPLAY "Error " CUST-STAT " on reread of file"
                                UPON CONSOLE
           ELSE
               DISPLAY "Read:" CM-CUST-NUM " unexpected."
           END-IF.
           REWRITE TSPFL-RECORD
           DISPLAY "Error " CUST-STAT " on Bad REWRITE"
                                UPON CONSOLE.
           DISPLAY "Started   ROLLBACK"
           ROLLBACK.
           DISPLAY "Completed ROLLBACK"
           MOVE SAV-KEY                      TO CM-CUST-NUM.
           READ  TSPFILE WITH LOCK
           IF CUST-STAT NOT = "00"
               DISPLAY "Error " CUST-STAT " not present "
                       CM-CUST-NUM " after Rollback" UPON CONSOLE
           ELSE
               DISPLAY "Read Key: " CM-CUST-NUM " " CM-COMPANY
                       " present after Rollback"
           END-IF.
           MOVE SAV-KEY                      TO CM-CUST-NUM.
           READ  TSPFILE WITH NO LOCK
           IF CUST-STAT NOT = "00"
               DISPLAY "Error " CUST-STAT " on reread "
                        CM-CUST-NUM " after Rollback" UPON CONSOLE
           END-IF.
           MOVE DATA-CUST-NUM (8)            TO CM-CUST-NUM.
           READ  TSPFILE WITH LOCK
           DISPLAY "Read Key: " CM-CUST-NUM " is " CM-COMPANY '.'.
           CLOSE TSPFILE.
           COMMIT.

])

