           MOVE "directory is file" TO LIST-TEST
           PERFORM LISTFILE.
           MOVE "./junky/chkdatax" TO DATAFILE.
           MOVE "invalid directory" TO LIST-TEST
           PERFORM LISTFILE.
           MOVE "/tmp" TO DATAFILE.
           MOVE "tmp directory" TO LIST-TEST
           PERFORM LISTFILE.
           STOP RUN RETURNING 0.

       LISTFILE.
           OPEN INPUT FLATFILE.
           IF CUST-STAT1 = '9'
              DISPLAY "List " LIST-TEST "Open Sts: 9/" CUST-STAT2
           ELSE
              DISPLAY "List " LIST-TEST "Open Sts: " CUST-STAT
           END-IF
           PERFORM UNTIL CUST-STAT NOT = "00"
               PERFORM READ-RECORD
           END-PERFORM.
           IF TST-WRITE = 'Y'
               MOVE 'N' TO TST-WRITE
               WRITE TSPFL-RECORD
               DISPLAY "Write INPUT file status " CUST-STAT
           END-IF.
           CLOSE FLATFILE.

       READ-RECORD.
           MOVE SPACES                       TO TSPFL-RECORD.
           READ FLATFILE
           IF  CUST-STAT NOT = "00"
           AND CUST-STAT NOT = "10"
             DISPLAY "Read Status: " CUST-STAT
           END-IF.

       LOADFILE.
           OPEN OUTPUT FLATFILE.
           IF CUST-STAT NOT = '00'
             IF CUST-STAT1 = '9'
                DISPLAY "Output Open Sts: 9/" CUST-STAT2
             ELSE
                DISPLAY "Output Open Sts: " CUST-STAT
             END-IF

           ELSE
             IF TST-READ = 'Y'
               MOVE 'N' TO TST-READ
               READ FLATFILE
               DISPLAY "Read OUTPUT file Status: " CUST-STAT
             END-IF

             PERFORM LOAD-RECORD
                        VARYING SUB FROM 1 BY 1
                          UNTIL SUB > MAX-SUB
                              OR CUST-STAT NOT = "00"

             CLOSE FLATFILE
           END-IF.

       LOAD-RECORD.

           MOVE SPACES                       TO TSPFL-RECORD.
           MOVE DATA-CUST-NUM      (SUB)     TO CM-CUST-NUM.
           MOVE DATA-COMPANY       (SUB)     TO CM-COMPANY.
           MOVE DATA-NO-TERMINALS  (SUB)     TO CM-NO-TERMINALS.
           MOVE 20070319                     TO CM-PK-DATE.
           IF SUB = 1 OR 4 OR 6
               MOVE -20070319                 TO CM-PK-DATE.

           IF  ODD-RECORD
               MOVE "8417"                   TO CM-DISK
           ELSE
               MOVE "8470"                   TO CM-DISK.
           WRITE TSPFL-RECORD.
           IF CUST-STAT NOT = "00"
               DISPLAY "Error status " CUST-STAT " writing record "
                       DATA-CUST-NUM (SUB).
])

