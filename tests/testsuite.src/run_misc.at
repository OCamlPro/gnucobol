#include <stdio.h>
#include <libcob.h>

extern int routine1(void *, void *);
extern int routine2(void *, void *);

int main()
{
    cob_init(0,NULL);
    routine1((void *)(0x1),(void *)(0x2));
    return 0;
}

void one_parameter(void *dummy)
{
   printf("Now in one_parameter\n");
    routine2((void *)(0x3),(void *)(0x4));
}
